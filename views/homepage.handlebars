{{#each posts as |post| }}
<div class="post">
    <div class="postUpper flexy toSides">
        <div class="postHeadline">
            {{post.headline}}
        </div>
        <div class="postInfo">
            Posted by {{post.user_id}}
        </div>
    </div>
    <div class="postLower">
        {{post.content}}
    </div>
</div>
{{/each}}