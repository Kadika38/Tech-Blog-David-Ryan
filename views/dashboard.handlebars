{{#if posts}}
{{#each posts as |post| }}
<div class="post">
    <div class="postUpper flexy toSides">
        <div class="postHeadline">
            {{post.headline}}
        </div>
        <div class="postInfo">
            Posted on {{post.user_id}}
        </div>
    </div>
    <div class="postLower">
        {{post.content}}
    </div>
</div>
{{/each}}
{{/if}}

{{#unless posts}}
<div>add posts!</div>
{{/unless}}

<script src="/js/dashboard.js"></script>